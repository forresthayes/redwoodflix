datasource DS {
  provider = "sqlite"
  url = "***"
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = env("BINARY_TARGET")
}

// Define your own datamodels here and run `yarn redwood db save` to create
// migrations for them.
model Movie {
  id          Int    @default(autoincrement()) @id
  title       String
  rating      String
  total_gross Int
}
